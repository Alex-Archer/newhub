.coupons {
  padding-top: 30rpx;
}

.coupons .item {
  height: 210rpx;
  border-radius: 20rpx;
  margin: 24rpx 40rpx;
  position: relative;
  overflow: hidden;
}

.couponList_bac {
  width: 100%;
  height: 210rpx;
}

.coupons .item:first-child {
  margin-top: 0;
}

.coupons .item .circle1,
.coupons .item .circle2 {
  width: 20rpx;
  height: 20rpx;
  background: #FBFAFA;
  position: absolute;
  left: 186rpx;
  border-radius: 100%;
  z-index: 10;
}

.coupons .item .m_a {
  width: 100%;
  height: 210rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.coupons .item .data_list {
  width: 120%;
  height: 210rpx;
}

.coupons .item .d_box {
  display: flex;
  justify-content: flex-start;
  justify-items: center;
  height: 100%;
}

.coupons .item .data {
  width: 100%;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  background: #fff;
}

.couponList_bac_on {
  width: 210rpx;
  height: 210rpx;
  background-color: #15CCBB;
  border-radius: 16rpx 0px 0px 16rpx;
  margin-right: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #fff;
}

.couponList_bac_on_right {
  width: 435rpx;
  padding-right: 30rpx;
}

.couponList_youxiaoqi {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color:#999;
  font-size:24rpx;
}
.couponList_youxiaoqi .using-rule{
  /* padding-left: 24rpx; */
}
.wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
}
.wrapper .btn{
  width: 80%;
  height: 88rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  line-height: 48rpx;
  background-color: #5ec9bb;
  color: #fff;
  border-radius: 48rpx;
  margin-top: 6rpx;
}
.wrapper .title{
  /* position: absolute; */
  flex-grow: 0;
 flex-shrink: 0;
 font-size: 36rpx;
}
.wrapper .text{
  /* position: absolute; */
 flex-grow: 1;
 flex-shrink: 0;
 padding: 50rpx 0;
 padding-left: 40rpx;
 font-size: 24rpx;
 flex-wrap: wrap;
 width: 100%;
}
.block {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: column;
  flex-grow: 0;
  flex-shrink: 0;
  padding: 35rpx 30rpx 30rpx 30rpx;
  width: 600rpx;
  /* height: 500rpx; */
  background-color: #fff;
  border-radius: 12rpx;
}
.couponList_lingqu {
  width: 116rpx;
  height: 52rpx;
  background-color: #FFBA15;
  box-shadow: 0px 2px 30px 0px rgba(10, 12, 19, 0.6);
  border-radius: 26rpx;
  text-align: center;
  line-height: 52rpx;
  color: #202531;
}

.couponList_bac_on_right_title {
  color: #333;
  font-weight: bold;
  font-size: 34rpx;
  margin: 0 0 10rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.couponList_bac_on_right_fanwei{
  color:#2c2c2c;
  font-size:24rpx;
  display: flex;
  justify-content: flex-start;
  margin-bottom: 10rpx;
}
.couponList_bac_on_spr{
  width: 280rpx;
}
.coupons .item .img {
  width: 196rpx;
  height: 210rpx;
  background: linear-gradient(-30deg, rgba(253, 216, 178, 1), rgba(216, 157, 97, 1));
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  text-align: center;
}

.coupons .item .img .money {
  color: #FFFFFF;
  font-size: 22rpx;
  line-height: 1;
  padding: 56rpx 0 24rpx;
}

.coupons .item .img .set {
  color: #FFFFFF;
  font-size: 24rpx;
  line-height: 1;
}

.coupons .item .cont {
  width: 494rpx;
  height: 210rpx;
  background: #FFFFFF;
  padding: 26rpx 0 0 28rpx;
}

.coupons .item .m_a.active .cont {
  border-radius: 20rpx;
}

.coupons .item .cont .tle {
  color: #2D2D2D;
  font-size: 26rpx;
  line-height: 34rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  width: 421rpx;
}

.coupons .item .cont .date {
  color: #343434;
  font-size: 22rpx;
  line-height: 1;
  padding: 15rpx 0 19rpx;
}

.coupons .item .cont .instructions {
  color: #999999;
  font-size: 22rpx;
  line-height: 28rpx;
  width: 690rpx;
  /* margin-top:-25rpx;
	background:#FBFAFA;
	border-radius:20rpx;
	color:#9A9A9A; */
  /* font-size:22rpx;
	line-height:28rpx;
	padding:42rpx 45rpx 30rpx 30rpx; */
}

.coupons .item .cont .info {
  color: #9A9A9A;
  font-size: 22rpx;
  line-height: 1;
  padding-right: 22rpx;
  display: inline-block;
  position: relative;
}

.coupons .item .cont .info .icon-down-jiantou {
  color: #999999;
  font-size: 16rpx;
  position: absolute;
  top: 50%;
  right: 0;
  margin-top: -8rpx;
}

.coupons .item .cont .info.on .icon-down-jiantou {
  transform: rotate(180deg);
  -webkit-transform: rotate(180deg);
}

.coupons .item .d_box .del {
  width: 166rpx;
  height: 210rpx;
  background: rgba(244, 73, 77, 1);
  border-radius: 0rpx 20rpx 20rpx 0rpx;
  line-height: 210rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 28rpx;
}


.wrap {
  min-height: 100%;
  position: relative;
}

.notBox {
  height: 876rpx;
  text-align: center;
  position: relative;
  padding-top: 228rpx;
}

.notBox image {
  width: 290rpx;
  height: 274rpx;
}

.notBox .not {
  font-size: 26rpx;
  line-height: 1;
  padding-top: 40rpx;
}

.bottom {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
}
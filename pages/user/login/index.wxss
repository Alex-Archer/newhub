.tui-line-cell {
	padding: 27rpx 0;
	display: -webkit-flex;
	display: flex;
	-webkiit-align-items: baseline;
	align-items:baseline;
	position: relative;
	box-sizing: border-box;
	overflow: hidden;
}

.tui-line-cell::after {
	content: '';
  width: 100%;
	position: absolute;
	/* border-bottom: 1px solid #e0e0e0; */
	-webkit-transform: scaleY(0.5) translateZ(0);
	transform: scaleY(0.5) translateZ(0);
	bottom: 0;
	right: 0;
	left: 0;
}
/* 同意协议 */

checkbox .wx-checkbox-input {
  margin: 6rpx 10rpx 0 10rpx;
  width: 40rpx;
  height:40rpx;
  border-radius: 10rpx;
  border: 1px solid #CCCCCC !important;
  display: inline-block;
  position: relative;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {/* 选中背景 */
  border-color: #fb9610;
  background-color:  #fb9610;
  border: 0px solid #CCCCCC !important;
  
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {/* 选中后勾号 */
  color: #fff;
  font-size: 32rpx;
  font-weight: bold;
  border: 0px solid #CCCCCC !important;
}
/* 同意协议 */
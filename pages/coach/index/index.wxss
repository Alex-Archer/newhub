/******banner图片切换*******/
.tui-banner-box {
  width: 100%;
  padding: 20rpx 0;
  box-sizing: border-box;
  background: #fff;
}
.tui-banner-swiper {
  width: 100%;
  height: 242rpx;
}
.tui-banner-item {
  padding: 0 16rpx;
  box-sizing: border-box;
}
.tui-slide-image {
  width: 100%;
  height: 242rpx;
  display: block;
  border-radius: 12rpx;
  transition: all 0.05s linear;
}
.tui-banner-scale {
  transform: scaleY(0.9);
  transform-origin: center center;
}
.tui-banner-swiper .wx-swiper-dot {
  width: 8rpx;
  height: 8rpx;
  display: inline-flex;
  background: none;
  justify-content: space-between;
}
.tui-banner-swiper .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(255, 255, 255, 0.8);
  border-radius: 16rpx;
  overflow: hidden;
}
.tui-banner-swiper .wx-swiper-dot-active::before {
  background: #fff;
}
.tui-banner-swiper .wx-swiper-dot.wx-swiper-dot-active {
  width: 16rpx;
}

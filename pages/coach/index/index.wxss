    left: 30rpx;
    z-index: 9999;
  }
/* 类型选择 */
  .tab-view {
    display: flex;
    flex-direction: column;
    height: 86rpx;
    position: relative;
    border-bottom: 0.5px solid #EAEAEA;
    background-color: white;
  }
  .tab-view .tabs {
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .tab-view .tabs .tab {
    display: flex;
    justify-content: center;

    width: 190rpx;
    font-size: 26rpx;
    color: #2c2c2c;
  }
  .tab-view .tabs .tab.typeactive {
    font-weight: bold;
    color: #EA5413;
  }
  .tab-view .line {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 48rpx;
    height: 2px;
    background-color: #EA5413;
    transition: 0.15s;
  }

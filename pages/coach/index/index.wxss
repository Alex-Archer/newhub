}
.tui-box .tui-content-text{
  padding: 10rpx 30rpx;
  overflow: hidden;
  text-overflow: ellipsis; 
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 26rpx; 
  line-height: 45rpx;
  color: #514a63;
  /* height: 400rpx; */
   /* 文字太长...号 */
   word-break: break-all;
   overflow: hidden;
   text-overflow: ellipsis;
   display: -webkit-box;

page {
    background: #fff;
  }
  
  .container {
    background-color: #fff;
    margin-top: 22rpx;
    padding: 30rpx;
    box-sizing: border-box;
    padding-bottom: 162rpx
  }
  
  .title {
    font-size: 30rpx;
    color: #666;
    padding: 24rpx 60rpx 24rpx 30rpx !important;
    font-weight: bold;
    margin-bottom: 10rpx;
  }
  .title-a {
    font-size: 30rpx;
    color: #666;
    font-weight: bold;
  
  }
  
  .tui-cells {
    border: 1rpx solid #e6e6e6;
    border-radius: 4rpx;
    height: 280rpx;
    box-sizing: border-box;
    padding: 20rpx 20rpx 0 20rpx;
    margin: 20rpx 20rpx 0 20rpx;
  }
  
  .tui-textarea {
    height: 210rpx;
    width: 100%;
    color: #666;
    font-size: 28rpx;
  }
  
  .pholder {
    color: #ccc;
  }
  
  .textarea-counter {
    font-size: 24rpx;
    color: #999;
    text-align: right;
    height: 40rpx;
    line-height: 40rpx;
    padding-top: 4rpx;
  }
  
  .top64 {
    margin-top: 64rpx;
  }
  .top32 {
      margin-top: 32rpx;
    }
  .tui-input {
  
    height: 88rpx;
    border: 1rpx solid #e6e6e6;
    border-radius: 4rpx;
    padding: 0 15rpx;
    margin: 10rpx 20rpx 0 20rpx;
    box-sizing: border-box;
  
  
    color: #666;
    font-size: 28rpx;
  }
  .tui-ptop{
      padding-top: 60rpx;
  }
  
  .tui-box-upload {
    padding-left: 25rpx;
    box-sizing: border-box;
  }
  
  
  .tui-list-cell {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 24rpx 60rpx 34rpx 30rpx !important;
    box-sizing: border-box;
    font-size: 30rpx;
  }
  
  .tui-content {
    font-size: 26rpx;
    color: rgb(158, 158, 158);
  }
  
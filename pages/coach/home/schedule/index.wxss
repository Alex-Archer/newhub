.container {
  padding: 0;
  padding-bottom: 190rpx;
  height: 100vh;
  width: 100vw;
  background-color: #fafafa;
}

/* ============日期选择模块===================== */
.select-data{
  height: 140rpx;
  box-sizing: border-box;
  width: 100%;

	display: flex;
	flex-direction: column;

  /* border-bottom: 1px solid #f6f6f6; */

  background-color: #fff;

  
 
}
.select-date-title{
  height: 130rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 40rpx 3% 40rpx 3%;
    box-sizing: border-box;
    border-bottom: 1px solid #f8f8f8;
    margin-bottom: 15rpx;

}
.select-date-title .title-date{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.title-date .date-day{
  color: #4184db;
  font-size:50rpx;
  margin-right: 16rpx;
}
.title-date .date-all
{
  display: flex;
  align-items:flex-start;
  flex-direction: column;
  font-size:26rpx;
  margin-right: 10rpx;
  color: #888;
}
 .date-all .day-week{
   margin-bottom: 6rpx;
 }
.select-date-title .title-menu{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #888;
}
.title-menu .menu-thisdate,.title-menu .menu-addclass
{
  margin: 0 0 0 20rpx;
}
.title-menu .menu-addclass
{
  /* color: #f74d54; */
}





.select-data .tui-line-h {
  position: relative;
  background-color: rgb(243, 243, 243);
  height: 1rpx;
}
.select-data .tui-line-h::after {
	content: '';
	position: absolute;
	border-bottom: 1rpx solid #eaeef1;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5);
	bottom: 0;
	right: 0;
	left: 0;
	transform-origin: 0 100%;
}


.select-data .tui-tab-item-active{
  background-color: #319fa6;/*#cbfcff;*/
  
  border-radius: 12rpx;
}


.select-data .tui-badge-item {/* 角标 */

  position: relative;
  margin-top: 20rpx; /* 不然角标出不来 */
}

.select-data .tui-tab-item-week
{
  font-size: 12px;
  color: #888;
	height: 30rpx;
	line-height: 30rpx;
	flex-wrap: nowrap;
  white-space: nowrap;
  display: flex;
  align-items: center;
  justify-content: center;
}
.select-data .tui-tab-item-title {
	color: #555;
	font-size: 30rpx;
  height: 60rpx;
  width: 60rpx;
	line-height: 60rpx;
	flex-wrap: nowrap;
  white-space: nowrap;
  font-weight: bold;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 20rpx 0 0 0;

}
.select-data  .tui-tab-item-title-active {/* 选中是圆形 */
  color: #fff;
  
  background: #319fa6;/*#ff7900;*/
  border-radius: 50%;
  /* border-bottom: 6rpx solid #fe8e61; 
  box-sizing: border-box;
  这是下划线*/
}
.tui-data-list {
  width: 100%;
  margin: 10rpx 0;
  /* padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between; */
}
/* 日期选择模块 End*/



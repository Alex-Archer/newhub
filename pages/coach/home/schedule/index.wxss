/* 日程区 */
.schedule-list-box {
  width: 96%;
  /* background: rgb(248, 248, 248); */
  overflow: hidden;
  margin: 10rpx 2%;
  box-sizing: border-box;
  
  
}
.schedule-list:not(:last-child)
{
  border-bottom: 1px solid #f6f6f6;
}
.schedule-list
{
  width:98%;
  margin: 10rpx 1% 20rpx 1%;
  padding: 0 0 20rpx 0;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
.schedule-list-t
{
  height: 250rpx;
  width: 100%;

  display: flex;
  box-sizing: border-box;
}


.color-a{
  border-left: 2px solid #319fa6;
}
.color-b{
  border-left: 2px solid #fe8e61;
}

.schedule-list-tl{
  display: flex;
  align-items: center;
  flex-direction: column;

  justify-content:center;
  width: 140rpx;
  flex: 2;
  
}
.list-tl-time{

  height: 40rpx;
  font-weight: bolder;
  font-size: 28rpx;
  margin: 4rpx 0;
}
.list-tl-split{
  
  height: 22rpx;
  font-size: 16rpx;
  color:#ccc;
}
.list-tl-state{
  
  height: 40rpx;
  font-size: 24rpx;
  color: #e0e0e0;
}
.schedule-list-tc{

  width: 1px;
  background-color: #efefef;
  margin: 30rpx 20rpx 30rpx 0;
  
}

.schedule-list-tr{
  display: flex;
  flex-direction: column;
  justify-content:center;
  flex: 7;
}
.list-tr-name,.list-tr-class,.list-tr-address{
  height: 50rpx;
  line-height: 50rpx;
}
.list-tr-name{
  display: flex;
  height: 50rpx;
  margin: 0 0 10rpx 0;
}
.list-tr-name .name{
  font-weight: bold;
  font-size: 30rpx;
}
.list-tr-name .tag{
  font-size: 24rpx;
  border-radius: 10rpx;
  background-color: #f1f1f0;
  height: 30rpx;
  line-height: 30rpx;
  margin: 7rpx 0 0 15rpx;
  padding: 5rpx 10rpx;
}
.list-tr-class{
  font-size: 28rpx;
  display: flex;
  height: 40rpx;
  margin: 0 0 10rpx 0;
  color: #666;


  
}
.list-tr-class .title{
  font-size: 24rpx;
  
}
.list-tr-class .section{
  font-size: 24rpx;
  padding: 0 20rpx;
    /* 文字太长...号 */
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;/* 行数 */
}
.list-tr-address{
  font-size: 24rpx;
  display: flex;
  color: #666;
}
.schedule-list-d{
  display: flex;
  align-items: center;
  justify-content:flex-end;

  height: 70rpx;
  margin-top:10rpx;

}
.schedule-list-d .sign,.schedule-list-d .student-sign-code
{
  height: 40rpx;
  line-height: 40rpx;
  color: #fe8e61;
  background-color: #fff;
  border: 1px solid #fe8e61;
  border-radius: 20rpx;
  padding: 6rpx 16rpx;
}
.schedule-list-d .student-sign-code
{
  margin-left: 20rpx;
}
.schedule-list-tel
{
  width:42rpx;
  height: 42rpx;
  line-height: 42rpx;

  flex-shrink: 0;

  display: flex;
  align-items: center;
  justify-content: center;
  margin: 20rpx 10rpx 0 ;
  flex: 1;
}
/* 日程区END */


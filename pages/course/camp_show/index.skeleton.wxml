<template name="skeleton">
  <view class="sk-container">
    <view class="container">
      <view class="tui-content-box_he">
        <swiper autoplay="false" circular="true" class="tui-banner-swiper" duration="150" indicator-active-color="#fff" indicator-color="rgba(255, 255, 255, 0.8)" indicator-dots="true" interval="5000" current="0">
          <swiper-item style="position: absolute; width: 100%; height: 100%; transform: translate(0%, 0px) translateZ(0px);">
            <image class="tui-slide-image sk-image" mode="widthFix" style="height: 281.5px;"></image>
          </swiper-item>
        </swiper>
        <view class="trainingCamp-data">
          <view class="trainingCamp-data_time">
            <text style="font-size: 17px; font-weight: bold;" class="sk-transparent sk-text-16-0000-122 sk-text">集训课程计划</text>
            <view class="trainingCamp-data-l">
              <text class="lesson sk-transparent sk-text-6-2500-312 sk-text">20课时</text>
              <text class="lesson-leave sk-transparent sk-text-6-2500-826 sk-text">仅剩 2 人开班</text>
              <view is="components/thorui/tui-tag/tui-tag">
                <view class="tag--tui-tag tag--tui-green sk-transparent sk-text-0-0000-18 sk-text" hover-class="true" hover-stay-time="150" style="transform:scale(1); padding: 4px 4px; margin: 0 10px 0 10px; font-size: 11px; line-height: 11px;background: #07c160;color: #fff;">报名中</view>
              </view>
            </view>
          </view>
          <view class="trainingCamp-data_price sk-transparent sk-text-14-2857-440 sk-text">¥0.04</view>
        </view>
        <view class="tui-box_he">
          <view class="coach">
            <text class="coach-title sk-transparent sk-text-14-2857-733 sk-text">时间：</text>
            <text class="coach-des sk-transparent sk-text-14-2857-651 sk-text">14:00~15:00</text>
          </view>
          <view class="coach">
            <text class="coach-title sk-transparent sk-text-14-2857-585 sk-text">日期：</text>
            <text class="coach-des sk-transparent sk-text-14-2857-854 sk-text">2023.11.28 - 2023.12.18</text>
          </view>
          <view class="coach" style="align-items: flex-start;">
            <text class="coach-title sk-transparent sk-text-14-2857-866 sk-text">地址：</text>
            <view class="coach_address">
              <view>
                <text class="coach-des sk-transparent sk-text-14-2857-561 sk-text">阳科园店</text>
                <text class="coach-des coach-location sk-transparent sk-text-14-2857-761 sk-text">江苏省苏州市昆山市博士路</text>
              </view>
              <view>
                <view is="components/ksald/ald-icon/ald-icon">
                  <text class="tn-icon-class ald-icon--tn-icon-class tn-icon-map ald-icon--tn-icon-map sk-pseudo sk-pseudo-circle" style="color: #666;font-size: 1.5rem;font-weight:normal;margin:0;padding:0;"></text>
                </view>
              </view>
            </view>
          </view>
          <view class="coach">
            <text class="coach-title sk-transparent sk-text-14-2857-174 sk-text">教室：</text>
            <text class="coach-des sk-transparent sk-text-14-2857-598 sk-text">大课普拉提教室</text>
          </view>
        </view>
        <view class="tui-box_he">
          <text class="title sk-transparent sk-text-14-2857-190 sk-text">课程介绍</text>
          <tui-overflow-hidden>
            <rich-text class="des-content" nodes="集训课的课程介绍。"></rich-text>
          </tui-overflow-hidden>
          <view class="tui-btn__box">
            <view is="components/thorui/tui-button/tui-button">
              <view class="tui-button--tui-button__wrap tui-button--tui-outline-fillet tui-button--tui-button__hover" style="width: 70px;height:23px;margin:20px auto">
                <button app-parameter="true" class="tui-button--tui-btn tui-button--tui-default-outline tui-button--tui-outline-fillet tui-button--tui-text-bold sk-transparent sk-button sk-pseudo sk-pseudo-circle" form-type="true" open-type="true" style="width: 70px; height: 23px; line-height: 23px; font-size: 11px;background:transparent;color: #319fa6;box-shadow: none;">展开全部</button>
                <view class="tui-button--tui-button__border tui-button--tui-outline-fillet" style="border-color:#319fa6"></view>
              </view>
            </view>
          </view>
        </view>
        <view class="tui-box_he">
          <text class="title sk-transparent sk-text-14-2857-115 sk-text">退营须知</text>
        </view>
      </view>
      <view class="tui-tabbar" style="padding-bottom: env(safe-area-inset-bottom);">
        <view class="tui-tabbar_pay">
          <view is="components/thorui/tui-button/tui-button" class="buttom-this">
            <view class="tui-button--tui-button__wrap tui-button--tui-btn__flex-1 tui-button--tui-fillet tui-button--tui-button__hover" style="width: 100%;height:40px;margin:0">
              <button app-parameter="true" class="tui-button--tui-btn tui-button--tui-btn-warning tui-button--tui-fillet sk-button sk-pseudo sk-pseudo-circle" form-type="true" open-type="true" style="width: 100%; height: 40px; line-height: 40px; font-size: 12px;background:#ff7900;color: #fff;box-shadow: none;flex-direction: column;">
                <view class="buttom-price sk-transparent sk-text-32-5000-435 sk-text" style="background-position-x: 50%;">立即购买集训营课程 ¥0.04</view>
                <view class="buttom-discount sk-transparent sk-text-35-0000-984 sk-text" style="text-decoration-color: transparent; background-position-x: 50%;">原价 ¥8888.00</view>
              </button>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>
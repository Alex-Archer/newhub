@import "./index.skeleton.wxss";
.container {
  padding-bottom: 98rpx;
}

.tui-box {
  padding: 20rpx 0 118rpx;
  box-sizing: border-box; 
}

.tui-address {
  min-height: 80rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
  position: relative;
}

.tui-userinfo {
  font-size: 30rpx;
  font-weight: 500;
  line-height: 30rpx;
  padding-bottom: 12rpx;
}

.tui-name {
  padding-right: 40rpx;
}

.tui-addr {
  font-size: 24rpx;
  word-break: break-all;
  padding-right: 25rpx;
}

.tui-addr-tag {
  padding: 5rpx 8rpx;
  flex-shrink: 0;
  background: #EB0909;
  color: #fff;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  font-size: 25rpx;
  line-height: 25rpx;
  transform: scale(0.8);
  transform-origin: 0 center;
  border-radius: 6rpx;
}

.tui-bg-img {
  position: absolute;
  width: 100%;
  height: 8rpx;
  left: 0;
  bottom: 0;
  background: url("data:image/png;base64,") repeat;
}

.tui-top {
  margin-top: 20rpx;
  overflow: hidden;
}

.tui-goods-title {
  font-size: 30rpx;
  display: flex;
  align-items: center;
  font-weight: bold;
}

.tui-padding {
  box-sizing: border-box;
}

.tui-goods-item {
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}

.tui-goods-img {
  width: 160rpx;
  height: 160rpx;
  display: block;
  flex-shrink: 0;
  margin: 10rpx 5rpx;
}

.tui-goods-center {
  flex: 1;
  padding: 20rpx 8rpx;
  box-sizing: border-box;
}

.tui-goods-name {
  max-width: 310rpx;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
  font-weight: bold;
  line-height: 32rpx;
}

.tui-goods-attr {
  font-size: 22rpx;
  color: #888888;
  line-height: 32rpx;
  padding-top: 20rpx;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.tui-price-right {
  text-align: right;
  font-size: 24rpx;
  color: #888888;
  line-height: 30rpx;
  padding-top: 20rpx;
}

.tui-flex {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
}
/* 排除需知 */
.tui-pd-title
{
  font-size: 30rpx;
  font-weight: bold;
  line-height:80rpx;
  height: 80rpx;

  display: flex;
  align-items: center;
  justify-content: space-between;
}
.tui-flex-pd {
  width: 100%;
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  line-height:45rpx;
  color: #989898;
  padding: 0 10rpx;
}
/* 排除需知END */

.tui-total-flex {
  justify-content: flex-end;
}

.tui-color-red,
.tui-invoice-text {
  color: #E41F19;
  padding-right: 30rpx;
}

.tui-color-none{
  color: #999;
}
.tui-color-default{
  color: rgb(0, 145, 101);
}
.tui-balance {
  font-size: 28rpx;
  font-weight:500;
}
.balance-title {
  font-size: 28rpx;
  font-weight:bolder;
}
.tui-balance-des
{
  font-size: 26rpx;
  line-height: 50rpx;
  color: #999;
}


.tui-black {
  color: #222;
  line-height: 30rpx;
}

.tui-gray {
  color: #888888;
  font-weight: 400;
}

.tui-light-dark {
  color: #666;
}

.tui-goods-price {
  display: flex;
  align-items: center;
  padding-top: 20rpx;
}

.tui-size-26 {
  font-size: 26rpx;
  line-height: 26rpx;
}

.tui-price-large {
  font-size: 34rpx;
  line-height: 32rpx;
  font-weight: 600;
}

.tui-flex-end {
  display: flex;
  align-items: flex-end;
  padding-right: 0;
}

.tui-phcolor {
  color: #B3B3B3;
  font-size: 26rpx;
}

.tui-remark-box {
  padding: 22rpx 30rpx;
}

.tui-remark {
  flex: 1;
  font-size: 26rpx;
  padding-left: 64rpx;
}

.tui-scale-small {
  transform: scale(0.7);
  transform-origin: 100% center;
}

.tui-scale-small .wx-switch-input {
  margin: 0 !important;
}





.tui-tabbar {
  width: 100%;
  height: 98rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 26rpx;
  box-shadow: 0 0 1px rgba(0, 0, 0, .3);
  padding-bottom: env(safe-area-inset-bottom);
  z-index: 996;
}

.tui-pr-30 {
  padding-right: 30rpx;
}

.tui-pr-20 {
  padding-right: 20rpx;
}

.tui-none-addr {
  height: 80rpx;
  padding-left: 5rpx;
  display: flex;
  align-items: center;
}

.tui-addr-img {
  width: 36rpx;
  height: 46rpx;
  display: block;
  margin-right: 15rpx;
}


.tui-pr25 {
  padding-right: 25rpx;
}

.tui-safe-area {
  height: 1rpx;
  padding-bottom: env(safe-area-inset-bottom);
}
/* 排除规则 */
.rule-box
{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 120rpx;
  margin: 20rpx 10rpx 30rpx 10rpx;
  box-sizing: border-box;
}
.rule-box-l
{
  flex: 2.5;
  flex-shrink: 0;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content:flex-start;
  font-size: 24rpx;
  margin-right: 20rpx;
  color: #888;
}
.rule-box-r
{
  flex: 8;
  flex-shrink: 1;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  box-sizing: border-box;
}
.rule-box-rt,.rule-box-rms
{
  display: flex;
  align-items: center;
  justify-content:space-between;

  width: 100%;
  box-sizing: border-box;

}

.rule-box-rmc
{
  height: 50rpx;
  line-height: 50rpx;
  width: 50%;
  text-align: center;
  font-size: 24rpx;
}
.rule-box-rt
{
  height: 20rpx;
  line-height: 20rpx;
}

.rule-box-circle
{
  width:16rpx;
  height: 16rpx;
  border-radius: 50%;
  
}
.rule-box-line
{
  width: 6rpx;
  height: 6rpx;
}
.circle-color-y
{
  background-color: #ffc800;
}
.circle-color-r
{
  background-color: #EB0909;
}
.color-y
{
  color: #ffc800;
}
.color-r
{
  color: #EB0909;
}
.rule-box-rd
{
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #888;
  font-size: 22rpx;
}
/* 排除规则END */

.tui-goods-user {
  font-size: 22rpx;
  color: #888888;
  line-height: 32rpx;
  padding-top: 20rpx;




  display: flex;
  align-items: center;

}

.goods-user-name,.goods-user-addres
{
  margin:0 0 0 4rpx;
    /* 文字太长...号 */
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;/* 行数 */
}
.goods-user-name
{
  margin:0 20rpx 0 4rpx;
  min-width: 60rpx;
}
.goods-user-date
{
  margin:0 20rpx 0 4rpx;
  min-width: 80rpx;
}


/* 购前必读 */
.tui-pd-agree
{
    font-size: 28rpx;
    color:#EB0909 ;
    flex-shrink: 0;
    height: 35rpx;
    line-height: 35rpx;
}
.tui-pd-radio
{
    height: 35rpx;
    line-height: 35rpx;
}
/*swtich整体大小及背景色*/
.wx-switch-input {
    width: 80rpx !important;
    height: 35rpx !important;
    box-sizing: border-box;
    position: relative;
    border: 0px !important;
}
   /* false的样式 */
  .wx-switch-input::before {
    width: 45px !important;
    height: 35rpx !important;
    background: #CCCCCC !important;
    border: 0px solid #CCCCCC !important;
    box-shadow: none !important;
    left: 0px;
    top: 0px;
  }
   /* 中间的圆 true样式 */
  .wx-switch-input::after {
    width: 40rpx !important;
    height: 35rpx !important;
    /* top: 2px !important;
    left: 2px !important; */
    opacity: 1 !important;
    box-sizing: border-box;
    box-shadow: none !important;
    border: 0px !important;
  }
  /* 购前必读 END */
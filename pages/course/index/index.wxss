/* 训练营 */

.camp-list
{
  width: 96%;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  margin: 10rpx 2%;

}
.trainingCamp-box{
  /* padding: 4% 5%; */
  /* padding: 0 0 70rpx 0; */
  margin:0 0 70rpx 0;
  width: 100%;
  box-sizing: border-box;
  height: auto;
}

.trainingCamp-box .trainingCamp-box_bg{
  position: absolute;
  width: 96%;
  border-radius: 15rpx;
  display: block;
  height: 280rpx;
  min-height: 280rpx;
  z-index: 1;
}
.trainingCamp-box .trainingCamp-box_mask{
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
  background-color: rgb(0, 0, 0 ,.6);
  display: block;
  height: 280rpx;
  width: 720rpx;
  z-index: 2;
}
.trainingCamp-box .trainingCamp-box_content{
  position: relative;
  z-index: 3;
}
.trainingCamp-box .trainingCamp-box_content .trainingCamp-box_person{
  float: right;
  background-color: #111111;
  border-radius: 0 15rpx 0 15rpx;
  font-size: 24rpx;
  color: #fceeb7;
  letter-spacing: 2rpx;
  padding: 8rpx 20rpx;
  margin: 0 0 0 0;
}
.trainingCamp-box .trainingCamp-box_content .trainingCamp-box_text{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between; 
  padding: 0 30rpx 0 50rpx;
  color: #ffffff;
  letter-spacing: 2rpx;
}
.trainingCamp-box .trainingCamp-box_content .trainingCamp-box_text .btn{
  width: 160rpx;
  border: 2px solid #ffffff;
  border-radius: 25rpx;
  background-color: transparent;
  font-size: 27rpx;
  font-weight: normal;
  padding: 7rpx 0;
  color: #ffffff;
  letter-spacing: 1rpx;
}
/* 训练营END */



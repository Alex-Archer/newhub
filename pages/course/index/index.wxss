/* ============顶部类型选择===================== */
.select-type{
  height: 100rpx;
  box-sizing: border-box;
  width: 100%;

  flex: 1;
	display: flex;
	flex-direction: column;
	overflow: hidden;
  background-color: #fafafa;
  /* height: 100%; */
  
  
}
/* 移动漂浮 */
.select-type.fixed{
  position:fixed;
  left: 0;
  top: 0;
  z-index: 999;
}


.select-type .tui-scroll-h {
	width: 100%;
	height: 80rpx;
	background-color: #ffffff;
	display: flex;
	flex-direction: row;
  white-space: nowrap;
  padding: 0 3% 0 0;
  box-sizing: border-box;
}

.select-type .tui-line-h {
  position: relative;
  background-color: rgb(243, 243, 243);
  height: 1rpx;
}
.select-type .tui-line-h::after {
	content: '';
	position: absolute;
	border-bottom: 1rpx solid #eaeef1;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5);
	bottom: 0;
	right: 0;
	left: 0;
	transform-origin: 0 100%;
}

.select-type .tui-tab-item {
	display: inline-block;
	flex-wrap: nowrap;
  padding:0 22rpx;
  
}

.select-type .tui-tab-item-title {
	color: #8a8696;
	font-size: 30rpx;
  height: 60rpx;
  /* width: 60rpx; */
	line-height: 60rpx;
	flex-wrap: nowrap;
  white-space: nowrap;
  font-weight: bold;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 20rpx 0 0 0;

}
.select-type .tui-tab-item-title-active {
  color: #29292b;
  font-size: 34rpx;
  border-bottom: 4rpx solid #fe8e61; 
  box-sizing: border-box;

}
/* ============顶部类型选择END===================== */

.page-title{
  padding: 30rpx; 
  font-size: 32rpx;
  font-weight: 600;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.page-title-r{
  font-size: 26rpx;
  color: #544e63;
  font-weight: normal;
  display: flex;
  margin-right: 10rpx;
}
.page-title-r-t{
  margin-right: 10rpx;
}
.personal-title{
  font-size: 32rpx;
  font-weight: 600;
}
.personal-title-more{
  font-size: 26rpx;
  color: #544e63;
}

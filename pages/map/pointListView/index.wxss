}

.tui-animation {
    display: inline-block;
    transform: rotate(0deg);
    transition: all 0.2s;
}

.tui-animation-show {
    transform: rotate(180deg);
}
.tui-selected-list {
    background-color: #fff;
    overflow: hidden;
    transform: translateZ(0);
    border-radius: 20rpx;
}

.tui-cell-class {
    display: flex;
    align-items: center;
    padding: 20rpx 20rpx !important;


}


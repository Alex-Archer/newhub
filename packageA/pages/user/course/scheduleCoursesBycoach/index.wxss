
.container {
    padding: 0;
    padding-bottom: 100rpx;
  
    height: 100vh;
    width: 100vw;
  }
  .tui-order-list {
	width: 100%;
	padding: 0 25rpx;
	margin-top: 40rpx;
	box-sizing: border-box;
}
.tui-order-item {
	margin-top: 20rpx;
	border-radius: 12rpx;
	box-shadow: 0 5rpx 10rpx 0 rgba(0, 0, 0, 0.06);
	overflow: hidden;
}

.tui-goods-title {
	width: 100%;
	font-size: 28rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
}

.schedule-title-box
{

    border-bottom: 1px solid #f0f0f0;
    padding-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
}
.schedule-title
{
    font-size: 30rpx;
    font-weight: bold;
}
.schedule-des
{
    font-size: 24rpx;
    color: rgb(173, 173, 173);

}
.schedule-item-box
{
    display: flex;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    margin: 20rpx 10rpx;
    align-items: center;
}
.class-item-li
{
    background-color: #f5f5f5;
    color: #333;
    height: 120rpx;
    width: 260rpx;
    border-radius: 20rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.li-title{
    font-size: 30rpx;
    font-weight: bold;
    line-height: 50rpx;
    height: 50rpx;

}
.li-des{
    font-size: 24rpx;
    line-height: 42rpx;
    height: 42rpx;
}
.class-item-li.active,.store-item-box.active{
    color: #ec6b4c;
    background-color: #fcefec;
}
/* 门店 */
.store-item-box
{
    
    margin-top:20rpx;
    width: 100%;
    height: 200rpx;
    background-color: #f5f5f5;
    color: #333;
    border-radius: 20rpx;
    display: flex;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    padding:15rpx 20rpx;
}
.store-item-box-none{
  margin-top:20rpx;
  width: 100%;
  height: 200rpx;
  background-color: #f5f5f5;
  color: #333;
  border-radius: 20rpx;
  display: flex;
  justify-content:center;
  flex-direction: column;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  padding:15rpx 20rpx;



}


.item-box-ico
{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    line-height: 50rpx;

}
.item-box-none{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  color:rgb(173, 173, 173);
  padding: 10rpx 0;

  font-size: 28rpx;
  line-height: 40rpx;

}
.item-box-animation{
    animation: scale 1s infinite;/* 定义keyframe动画，命名为blink */
    -webkit-animation: scale .5s 1;/* 定义keyframe动画，命名为blink */

}
/* 定义keyframe动画，命名为blink */
@keyframes scale{
    0%{color: rgb(173, 173, 173);font-size: 28rpx;}
    50%{color: rgb(173, 173, 173);font-size: 30rpx;}
    100%{color: rgb(173, 173, 173);font-size: 28rpx;}
  }
  
  @-webkit-keyframes scale{
    0%{color: rgb(173, 173, 173);font-size: 28rpx;}
    50%{color: rgb(173, 173, 173);font-size: 30rpx;}
    100%{color: rgb(173, 173, 173);font-size: 28rpx;}
  }

.item-box-l
{
    width: 170rpx;
    height: 170rpx;
    flex:2;
}
.item-box-r
{

    flex:5;
    display: flex;
    flex-direction: column;
    padding-left: 20rpx;

}
.store-img
{
    width: 170rpx;
    height: 170rpx;
    border-radius: 20rpx;
}
.box-r-title
{
    height: 80rpx;
    line-height: 40rpx;

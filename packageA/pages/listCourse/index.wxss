.tui-goods-name {
	font-size: 32rpx;
  line-height: 36rpx;
  font-weight: bolder;
  margin-bottom: 14rpx;
  display: flex;

  /* text-decoration:line-through; */

}
.tui-goods-name.baseline{
    align-items:baseline;
}
.class-all
{
  color: #555;
  font-size: 34rpx;
  
}
.class-all.red
{
  color: red;

}

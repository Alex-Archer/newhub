.tui-userinfo-box {
    margin: 20rpx 0;
    color: #333;
}

.tui-list-cell,.tui-list-cell-switch {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    
    box-sizing: border-box;
    font-size: 30rpx;
}
.tui-list-cell{
    padding: 24rpx 60rpx 24rpx 30rpx !important;
}
.tui-list-cell-switch {
    padding: 24rpx 20rpx 24rpx 30rpx !important;
}
.face-des
{
    height: 40px;
    line-height: 40px;
    padding: 0 0 20px 0;
    color: #666;
    font-weight: normal;
    display: flex;
    align-items: center;
    flex-direction: column;

}
.tui-pr30 {
    padding-right: 30rpx;
}

.tui-avatar {
    width: 100rpx;
    height: 100rpx;
    display: block;
    border-radius: 50%;
}

.tui-content {
    font-size: 26rpx;
    color: #666;
}


.avatar-wrapper::after {
    border: none;
}

.avatar-wrapper:not([size='mini']) {
    width: 100vw;
    margin: 0;
    padding: 0;
}
/*人脸模块*/

.face-box
{

    width: 400rpx;
    min-height: 400rpx;
    padding: 30rpx 0;
   position: relative;
   display: flex;
   align-items: center;
   justify-content: center;

   margin: auto auto;
   
}
.face-del
{
    
    position:absolute;
    top:40rpx;
    right: 20rpx;
    width: 40rpx;
    height: 40rpx;
    background-color: #fff;
    border-radius: 50%;
    padding: 14rpx;
    display: flex;
   align-items: center;
   justify-content: center;

}
.face-img
{
    width: 100%;
    

    display: block;

}
.tui-list-cell-des
{
    width: 100%;
    display: flex;
    flex-direction: column;
   
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    width: 100%;
    padding: 0 30rpx;
    color: rgb(184, 184, 184);
   
}
.use-des-box
{
    width: 100%;
    line-height: 50rpx;
    display: flex;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    flex-direction: column;
    padding-top: 20rpx;
}
.content-pass
{
    font-size: 34rpx;
    letter-spacing: 6rpx;
    font-weight: bold;

}
.use-des-title
{
    font-weight: bold;
}
.use-des
{
    width: 100%;
    padding-left: 20rpx;
    line-height: 60rpx;

}

/* 密码面板 */

.tui-keyboard-tips {
    width: 100%;
    height: 120rpx;
    line-height: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 30rpx;
    background: #fff;
    position: relative;
    color: #333;
    border: 0;
}

.tui-digital-box {
    background: #fff;
    padding-bottom: 50rpx;
    border: 0;
}
